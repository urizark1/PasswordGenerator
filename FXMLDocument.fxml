<AnchorPane id="AnchorPane" prefHeight="468.0" prefWidth="376.0" 
            style="-fx-background-color: black;" 
            xmlns="http://javafx.com/javafx/8.0.111" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="githubproject.FXMLDocumentController">
    
   <children>
       
      <Label layoutX="73.0" layoutY="66.0" prefHeight="35.0" prefWidth="230.0"
             text="Password Generator" textFill="#556794">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      
      <Button fx:id="button" layoutX="138.0" layoutY="299.0" 
              mnemonicParsing="false" onAction="#handleButtonAction" 
              text="Generate" textFill="#556794">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      
      <TextField fx:id="txtField" layoutX="114.0" layoutY="174.0" />
      
   </children>
   
</AnchorPane>
